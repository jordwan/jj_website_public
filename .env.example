# Rename this file to .env and fill in your values
# NEVER commit the actual .env file to version control!

# Your Ethereum node URL (e.g., from Alchemy or Infura)
GOERLI_URL=https://eth-goerli.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Your wallet private key (KEEP THIS SECRET!)
# DO NOT share this key or commit it to version control
PRIVATE_KEY=your_wallet_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_KEY=your_etherscan_api_key_here

# Frontend-safe environment variables (these will be exposed to the client)
# Contract address should be added here after deployment
REACT_APP_CONTRACT_ADDRESS=0x_your_deployed_contract_address